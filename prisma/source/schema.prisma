generator client {
  provider = "prisma-client-js"
  output   = "../../src/generated/source" // Define onde o client ser√° gerado
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_SOURCE_URL")
}


model User {
  id    Int     @id @default(autoincrement())
  email String  @unique
  name  String?
}